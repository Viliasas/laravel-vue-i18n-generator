{
    "name": "viliasas/laravel-vue-i18n-generator",
    "license": "MIT",
    "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
    "keywords": [
        "laravel",
        "vue-i18n"
    ],
    "homepage": "http://github.com/viliasas/laravel-vue-i18n-generator",
    "authors": [
        {
            "name": "Vilius Maƒçiulis",
            "email": "viliasas@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/console": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Viliasas\\VueInternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viliasas\\VueInternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
